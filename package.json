{
  "name": "uci-console",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "jest",
    "test:ci": "jest --silent --ci --w 2 --collectCoverage=true --coverageReporters=lcov",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "copy:assets": "cp -r ./projects/uci-console/assets dist/uci-console/",
    "build-lib": "ng build --configuration production && npm run copy:assets",
    "build-doc": "compodoc -p tsconfig.doc.json"
  },
  "dependencies": {
    "@angular/animations": "^16.2.12",
    "@angular/cdk": "16.2.12",
    "@angular/common": "^16.2.12",
    "@angular/compiler": "^16.2.12",
    "@angular/core": "^16.2.12",
    "@angular/forms": "^16.2.12",
    "@angular/localize": "^16.2.12",
    "@angular/material": "16.2.12",
    "@angular/platform-browser": "^16.2.12",
    "@angular/platform-browser-dynamic": "^16.2.12",
    "@angular/router": "^16.2.12",
    "@project-sunbird/common-form-elements-full": "8.0.1",
    "copyfiles": "^2.4.1",
    "core-js": "^2.5.4",
    "dayjs": "^1.10.4",
    "moment": "^2.29.1",
    "@project-sunbird/ng2-semantic-ui": "8.0.2",
    "ngx-chips": "^3.0.0",
    "nodemon": "^2.0.7",
    "rxjs": "~6.6.7",
    "tslib": "^2.0.0",
    "zone.js": "~0.13.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.13",
    "@angular/cli": "^16.2.13",
    "@angular/compiler-cli": "^16.2.12",
    "@angular/language-service": "^16.2.12",
    "@project-sunbird/client-services": "5.1.2",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/jest": "^26.0.22",
    "@types/node": "^12.11.1",
    "compodoc": "^0.0.41",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~5.0.0",
    "jest": "^29.7.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lodash-es": "^4.17.20",
    "ng-packagr": "^16.2.3",
    "protractor": "~7.0.0",
    "ts-jest": "^29.1.2",
    "ts-node": ">=9.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.9.5"
  },
  "description": "Samagra UCI-Console Frontend Module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samagra-development/uci-console.git"
  },
  "keywords": [
    "sunbird",
    "discussion",
    "angular"
  ],
  "author": "Bharat Kashyap",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/samagra-development/uci-console/issues"
  },
  "homepage": "https://github.com/samagra-development/uci-console#readme",
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfig": "<rootDir>/projects/uci-console/tsconfig.spec.json"
      }
    },
    "browser": false,
    "rootDir": "./",
    "roots": [
      "<rootDir>/projects/uci-console/src/"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "coveragePathIgnorePatterns": [],
    "testMatch": [
      "**/?(*.)(spec).ts"
    ],
    "restoreMocks": true,
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "ts",
      "tsx"
    ],
    "setupFiles": [],
    "transform": {
      "^.+\\.(ts)$": "ts-jest"
    },
    "coverageReporters": [
      "text",
      "json"
    ],
    "transformIgnorePatterns": []
  }
}